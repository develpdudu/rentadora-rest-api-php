    "keywords": [
        "php",
        "rest-api",
        "mysql",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/develpudu/rentadora-rest-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Devel Pudu",
