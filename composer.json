            "homepage": "https://github.com/develpudu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "vlucas/phpdotenv": "^5.2",
        "firebase/php-jwt": "^5.0"
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0,
        "platform": {
            "php": "7.3.0"
